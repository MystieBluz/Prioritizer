 <div class="timeremaining">
            <p>Voting in Progress</p>
            <p>10:42 Remaining</p>
        </div>
        <div class="user">
            <p>User: Michael</p>
        </div>
        <div class="votes">
            <p><span class="votescast">3</span> Votes cast<span class="votesremaining"> 2</span> Votes remaining</p>

  
      <div class="col-12 mainideas" height="400">
        <table class="table table-bordered">
            <thead>
              <tr>
                <th scope="col">ID</th>
                <th scope="col">Proposal</th>
                <th scope="col">Votes</th>
                <th scope="col"></th>
                <th scope="col"></th>
               </tr>
            </thead>
            <tbody>
                <!-- Loop over each object in the proposals array  -->
                {{#each proposals as |proposal|}}
              <tr>
                <td id="{{proposal.id}}">{{proposal.id}}</td>
                <td id="p{{proposal.id}}">{{proposal.proposal}}</td>
                <td id="pv{{proposal.id}}">{{proposal.prop_votes}}</td>
                <td>  
                {{#times 5}} 
                  
                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-star" viewBox="0 0 16 16">
