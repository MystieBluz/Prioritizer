  
      <div class="col-7 mainideas" height="400">
        <table class="table table-bordered">
            <thead>
              <tr>
                <th scope="col">ID</th>
                <th scope="col">Proposal</th>
                <th scope="col">Final Vote Count</th>
               </tr>
            </thead>
            <tbody>
                <!-- Loop over each object in the proposals array  -->
                {{#each proposals as |proposal|}}
              <tr>
                <td id="{{proposal.id}}">{{proposal.id}}</td>
                <td id="p{{proposal.id}}">{{proposal.proposal}}</td>
                <td id="pv{{proposal.id}}">{{proposal.prop_votes}}</td>
              </tr>
              {{/each}}
            </tbody>
          </table>
          </div> 

           <div id="chartHolder" class="col-5" height="400">
            <canvas id="myChart" width="400" height="400"></canvas>
          </div>
        </row>
       
    <script src="../js/chart.js"></script>
